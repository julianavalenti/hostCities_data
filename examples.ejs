<!DOCTYPE html>
<html>
<head>
    <title>Host Cities</title>
</head>
<body>
    <h1>Host Cities</h1>
    <% cities.forEach(city => { %>
        <h2><%= city.city %></h2>
        <ul>
            <% city.mustSee.forEach(mustSee => { %>
                <li><%= mustSee.name %></li>
                <li><%= mustSee.admissionFee %></li>
                <li><%= mustSee.category %></li>
                <li><%= mustSee.website %></li>
            <% }); %>
        </ul>
    <% }); %>
</body>
</html>


<style>
    body {
        background-image: url("/images/dreamstime_l_136805319.jpg");
        background-attachment:fixed;
        background-position: center top;
        background-repeat: no-repeat;
    }

</style>

<form class="form-horizontal">
    <fieldset>
    
    <!-- Form Name -->
    <legend>Form Name</legend>
    
    <!-- Text input-->
    <div class="form-group">
      <label class="col-md-4 control-label" for="Name">Name</label>  
      <div class="col-md-5">
      <input id="Name" name="Name" type="text" placeholder="Full name" class="form-control input-md" required="">
        
      </div>
    </div>
    
    <!-- Text input-->
    <div class="form-group">
      <label class="col-md-4 control-label" for="phone">Phone</label>  
      <div class="col-md-4">
      <input id="phone" name="phone" type="text" placeholder="e.g (555) 555 - 5555" class="form-control input-md">
      <span class="help-block">help</span>  
      </div>
    </div>
    
    <!-- Text input-->
    <div class="form-group">
      <label class="col-md-4 control-label" for="email">Email</label>  
      <div class="col-md-5">
      <input id="email" name="email" type="text" placeholder="e.g user@example.com" class="form-control input-md" required="">
        
      </div>
    </div>
    
    <!-- Password input-->
    <div class="form-group">
      <label class="col-md-4 control-label" for="password">Password </label>
      <div class="col-md-5">
        <input id="password" name="password" type="password" placeholder="" class="form-control input-md" required="">
        
      </div>
    </div>
    
    </fieldset>
    </form>
    